ALERT DSEHighCPULoad 
  IF node_load1{job="cassandra"} > 16
  FOR 10m
  LABELS {
    service = "dse-cassandra",
    severity = "warning",
    spark = "false",
    monitored_item = `{{$labels.instance}}/{{$labels.job}}`,
  }
  ANNOTATIONS {
    summary = "Node in env {{$labels.environment}} - {{$labels.instance}} is reporting a high CPU load average",
    description = "Node had a CPU load average (1m) above 16 for 10 minutes: {{$value}}",
  }
