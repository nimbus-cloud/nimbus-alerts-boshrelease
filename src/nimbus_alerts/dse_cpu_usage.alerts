ALERT DSEHighCPULoad 
  IF node_load1{job="cassandra"} > 20
  FOR 10m
  LABELS {
    service = "cassandra",
    severity = "warning",
  }
  ANNOTATIONS {
    summary = "Node in env {{$labels.environment}} - {{$labels.instance}} is reporting a high CPU load average",
    description = "Node had a CPU load average (1m) above 20 for 10 minutes: {{$value}}",
    product = "OCP"
  }


