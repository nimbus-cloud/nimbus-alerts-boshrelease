ALERT DSEUnstableNFS
  IF node_memory_NFS_Unstable{job="cassandra"} > 0 
  FOR 10m
  LABELS {
    service = "cassandra",
    severity = "warning",
  }
  ANNOTATIONS {
    summary = "Node in DSE is experincing unstable NFS",
    description = "Node in DSE is experincing unstable NFS",
    product = "OCP"
  }

